{
  "name": "react-instrument",
  "description": "Looks after UI and and sequencing of your instrument, and lets you have fun building the sound engine",
  "main": "dist/Instrument/index.js",
  "version": "0.1.3",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@types/jest": "24.0.9",
    "@types/node": "11.10.4",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "fluent-react-play-button": "1.0.7",
    "material-ui": "^0.20.2",
    "nanoevents": "^1.0.8",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-piano": "^3.1.2",
    "react-rnd": "^9.1.2",
    "react-scripts": "2.1.5",
    "styled-components": "^4.1.3",
    "tunajs": "^1.0.9",
    "typescript": "^3.3.3333",
    "webaudio-peaks": "0.0.6",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "babel --out-dir dist src --presets=@babel/preset-env,@babel/preset-react,@babel/preset-typescript --extensions \".ts\",\".tsx\",\".js\",\".jsx\",\".es6\",\".es\",\".css\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "babel-loader": "8.0.5",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "react-app-rewired": "^2.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css,md}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*"
    ]
  }
}
